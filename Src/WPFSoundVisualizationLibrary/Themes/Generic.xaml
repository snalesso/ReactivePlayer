    <!--  Region: Equalizer  -->
    <Style TargetType="{x:Type local:Equalizer}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:Equalizer}">
                    <ControlTemplate.Resources>
                        <!--  Region Slider  -->
                        <Style
                            x:Key="EqualizerSliderThumb"
                            BasedOn="{StaticResource {x:Type Thumb}}"
                            TargetType="{x:Type Thumb}">
                            <Setter Property="SnapsToDevicePixels" Value="true" />
                        </Style>
                        <Style
                            x:Key="EqualizerSlider"
                            BasedOn="{StaticResource {x:Type Slider}}"
                            TargetType="{x:Type Slider}">
                            <Setter Property="Orientation" Value="Vertical" />
                            <Setter Property="HorizontalAlignment" Value="Center" />
                        </Style>
                        <!--  EndRegion  -->
                    </ControlTemplate.Resources>
                    <Border
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid Name="MainGrid">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="30" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <TextBlock
                                Name="LowText"
                                Margin="10,2,2,2"
                                HorizontalAlignment="Left"
                                Text="Low" />
                            <TextBlock
                                Name="MidText"
                                Margin="2"
                                HorizontalAlignment="Center"
                                Text="Mid" />
                            <TextBlock
                                Name="HighText"
                                Margin="2,2,10,2"
                                HorizontalAlignment="Right"
                                Text="High" />
                            <Grid Name="PART_EqualizerGrid" Grid.Row="1" />
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--  EndRegion: Equalizer  -->

</ResourceDictionary>
